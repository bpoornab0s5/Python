class Solution:
    def containsDuplicate(self, nums: List[int]) -> bool:
        # declaring a empty set to store the values in an array
        num = set()
        #selecting each valuse from the array
        for i in nums:
        #checking if the selected valuse is in the set if yes then return true
            if i in num:
                return True
#after checking adding the valuse to the set anf this is looped until end of array
            num.add(i)
        return False
